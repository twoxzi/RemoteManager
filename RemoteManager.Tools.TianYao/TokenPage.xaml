<Window x:Class="Twoxzi.RemoteManager.Tools.TianYao.TokenPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Twoxzi.RemoteManager.Tools.TianYao"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="令牌设置">

    <StackPanel>
        <Button Content="OpenFile" Command="{Binding OpenFileCommand}"/>
        <TextBlock TextWrapping="Wrap" Text="{Binding RemoteInfo.ExtensionProperty}"></TextBlock>
    </StackPanel>
</Window>
